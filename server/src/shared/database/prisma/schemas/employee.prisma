model Employee {
  id             String        @id @default(auto()) @map("_id") @db.ObjectId
  name           String        @unique
  commissions    Commission[]  @relation(fields: [comissionIds], references: [id])
  comissionIds   String[]      @db.ObjectId
  serviceTypes   ServiceType[] @relation(fields: [serviceTypeIds], references: [id])
  serviceTypeIds String[]      @db.ObjectId
  createdAt      DateTime      @default(now())
  updatedAt      DateTime      @updatedAt

  @@map("employees")
}
